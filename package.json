{
  "name": "portal-catalog-assets",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "pnpm generate:navigation && vite --mode staging --port 3000 --force",
    "preview": "vite preview --port 3001",
    "transpile": "tsc -p tsconfig.json",
    "prebuild": "pnpm generate:navigation && pnpm transpile",
    "build:development": "pnpm prebuild && vite build --mode dev",
    "build:staging": "pnpm prebuild && vite build --mode staging",
    "build:production": "pnpm prebuild && vite build --mode production",
    "generate:navigation": "citron --useHash=false",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "check-branch": "pnpm generate:navigation && pnpm lint && pnpm build:development",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ci": "vitest run --coverage"
  },
  "dependencies": {
    "@oazapfts/runtime": "^1.0.4",
    "@stack-spot/auth": "~6.0.0",
    "@stack-spot/auth-react": "~2.13.0",
    "@stack-spot/citric-icons": "^0.3.0",
    "@stack-spot/citric-react": "^0.43.0",
    "@stack-spot/citron-navigator": "~1.7.0",
    "@stack-spot/opa": "~2.8.1",
    "@stack-spot/portal-components": "2.27.5",
    "@stack-spot/portal-layout": "2.38.1",
    "@stack-spot/portal-mfe-utils": "^1.0.1",
    "@stack-spot/portal-network": "0.204.1",
    "@stack-spot/portal-observability": "^0.8.0",
    "@stack-spot/portal-theme": "~1.2.1",
    "@stack-spot/portal-translate": "~2.1.0",
    "@tanstack/react-query": "^5.85.5",
    "lodash": "npm:lodash-es@4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "6.1.10"
  },
  "devDependencies": {
    "@originjs/vite-plugin-federation": "npm:@stack-spot/vite-plugin-federation@1.3.6",
    "@stack-spot/citron-navigator-cli": "~1.2.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash": "^4.17.20",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "eslint": "^8.57.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.19",
    "make-dir-cli": "^4.0.0",
    "oazapfts": "^6.2.2",
    "prettier-eslint": "^16.4.2",
    "typescript": "^5.8.2",
    "vite": "^5.4.19",
    "vite-plugin-preload": "^0.4.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.1.9"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@9.2.0"
}