{
  "name": "oazapfts",
  "version": "6.4.0",
  "description": "OpenApi TypeScript client generator",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oazapfts/oazapfts",
    "directory": "packages/codegen"
  },
  "type": "module",
  "main": "index.cjs",
  "types": "index.d.ts",
  "bin": {
    "oazapfts": "cli.js"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.cjs",
      "default": "./index.js"
    },
    "./generate": {
      "types": "./generate.d.ts",
      "require": "./generate.cjs",
      "default": "./generate.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist && vite build --mode esm && vite build --mode cjs && chmod +x cli.js && chmod +x cli.cjs",
    "prepack": "npm run build && sed -i 's|||g' package.json && mv * ."
  },
  "release": {
    "extends": "semantic-release-monorepo",
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.17.21",
    "@types/minimist": "^1.2.5",
    "@types/tapable": "^2.3.0"
  },
  "peerDependencies": {
    "@oazapfts/runtime": "*"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^12.1.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "swagger2openapi": "^7.0.8",
    "tapable": "^2.3.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "openapi",
    "typescript",
    "codegen",
    "oazapfts"
  ]
}
